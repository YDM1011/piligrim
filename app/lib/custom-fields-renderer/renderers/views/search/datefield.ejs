<div class="users-head__td  has-label" ng-if="showedFilters.indexOf('<%= fieldConfig.name; %>') > -1">
  <label translate><%= fieldConfig.label %></label>
  <a ng-click="removeShowedFilter('<%= fieldConfig.name; %>')" class="remove-filter-field">
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="9887.563 -13109.671 11.875 10.342">
      <g id="Group_2373" data-name="Group 2373" transform="translate(8342 -13760)">
        <g id="Group_1172" data-name="Group 1172" class="cls-1" transform="translate(5 7)">
          <line id="Line_297" data-name="Line 297" class="cls-2" x2="10" y2="8" transform="translate(1541.5 644.5)"></line>
          <line id="Line_298" data-name="Line 298" class="cls-2" y1="8" x2="10" transform="translate(1541.5 644.5)"></line>
        </g>
      </g>
    </svg>
  </a>
  <div ng-controller="DateFilterCtrl" class="rc-select select-adding">
    <ui-select ng-init="$parent.searchFilterModel['<%= fieldConfig.name; %>'].operator = 'equals'"ng-model="$parent.searchFilterModel['<%= fieldConfig.name; %>'].operator" theme="select2" search-enabled="false">
      <ui-select-match>{{$select.selected.label}}</ui-select-match>
      <ui-select-choices repeat="item.value as item in drodownValues">
        <div ng-bind-html="item.label"></div>
      </ui-select-choices>
    </ui-select>
  </div>
  <div class="users-head__td--second-row" ng-if="['afterXDays', 'olderThanXDays', 'nextXDays', 'lastXDays'].indexOf(searchFilterModel['<%= fieldConfig.name; %>'].operator) > -1">
    <input type="number" class="form-input" ng-model="searchFilterModel['<%= fieldConfig.name; %>'].countDays" placeholder="{{'Days' | translate}}">
  </div>
  <div class="users-head__td--second-row" ng-if="['on', 'after', 'before'].indexOf(searchFilterModel['<%= fieldConfig.name; %>'].operator) > -1 && getDateFormat() && getTimezone()">
    <input type="text" date-time class="form-input input-date" ng-model="searchFilterModel['<%= fieldConfig.name; %>'].value" placeholder="{{'Date' | translate}}" view="date" auto-close="true" min-view="date" format="{{getDateFormat()}}" timezone="{{getTimezone()}}" position="absolute">
  </div>
  <div class="users-head__td--second-row col-6 from--date" ng-if="['between'].indexOf(searchFilterModel['<%= fieldConfig.name; %>'].operator) > -1 && getDateFormat() && getTimezone()">
    <input type="text" date-time class="form-input input-date" ng-model="searchFilterModel['<%= fieldConfig.name; %>'].from" placeholder="{{'Date from' | translate}}" view="date" auto-close="true" min-view="date" format="{{getDateFormat()}}" timezone="{{getTimezone()}}" position="absolute">
  </div>
  <div class="users-head__td--second-row  col-6 to--date" ng-if="['between'].indexOf(searchFilterModel['<%= fieldConfig.name; %>'].operator) > -1 && getDateFormat() && getTimezone()">
    <input type="text" date-time class="form-input input-date" ng-model="searchFilterModel['<%= fieldConfig.name; %>'].to" placeholder="{{'Date to' | translate}}" view="date" auto-close="true" min-view="date" format="{{getDateFormat()}}" timezone="{{getTimezone()}}" position="absolute">
  </div>
  <div class="wl-clearfix"></div>
</div>
