<div class="col-6">
  <div class="contact-adding__item">
    <label class="contact-adding__item-label" translate><%= fieldConfig.label %></label>
    <div class="contact-adding__item-body">
      <div class="row">
        <div class="col-12 rc-select rc-select--multiple">
          <select class="form-control chosen-select" chosen multiple="multiple" data-placeholder=" " class="form-control" ng-model="formModel.productWishes.<%= fieldConfig.name; %>.values" id="<%= fieldConfig.module %>_dropdown_filter_<%= fieldConfig.name %>">
            <% options.forEach(function(option){ %>
              <option value="<%= option.value %>" translate><%= option.label %></option>
            <% }); %>
          </select>
        </div>
      </div>
    </div>
  </div>
</div>
