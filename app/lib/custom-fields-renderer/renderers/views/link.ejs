<div class="pos-rel rc-select col-<%= fieldConfig.colSize %>"   init-default-value data-module="<%= fieldConfig.module %>" data-name="<%=fieldConfig.name %>" <% if (fieldConfig.minHeigth) { %> style="min-height: <%= fieldConfig.minHeigth  %>px;" <% } %> >
  <label class="contact-adding__item-label <% if (fieldConfig.isRequired) { %>is-required<% } %>" translate><%= fieldConfig.label %></label>
  <% if (fieldConfig.hasTooltip) { %>
  <span class="b-r-xl" tooltip-placement="top" uib-tooltip-template="'/api/customField/tooltip/<%= fieldConfig.module %>/<%=fieldConfig.name %>?_=<%= Date.now() %>'"><i class="fa fa-info-circle"></i></span>
  <% } %>
  <custom-link-list model="<%= fieldConfig.options ? fieldConfig.options.module : '' %>" field-options='<%- JSON.stringify(fieldConfig.options) %>' label="<%= labelField %>">
    <% if (multiple) { %>
      <ui-select multiple ng-model="formModel.<%= fieldConfig.name %>.itemId" id="<%= fieldConfig.module %>_dropdown_<%= fieldConfig.name %>" theme="select2" append-to-body="true">
        <ui-select-match>{{$item.<%=labelField%>}}</ui-select-match>
        <ui-select-choices repeat='item._id as item in linkCustomFieldList.<%= fieldConfig.options ? fieldConfig.options.module : 'undefined' %>'>
          <div>{{item.<%=labelField%>}}</div>
        </ui-select-choices>
      </ui-select>
    <% } else { %>
      <ui-select ng-model="formModel.<%= fieldConfig.name %>.itemId" id="<%= fieldConfig.module %>_dropdown_<%= fieldConfig.name %>" theme="select2" append-to-body="true">
        <ui-select-match>{{$select.selected.<%=labelField%>}}</ui-select-match>
        <ui-select-choices repeat='item._id as item in linkCustomFieldList.<%= fieldConfig.options ? fieldConfig.options.module : 'undefined' %>'>
          <div>{{item.<%=labelField%>}}</div>
        </ui-select-choices>
      </ui-select>
    <% } %>
  </custom-link-list>
  <validation-error field="'<%= fieldConfig.name %>'" errors="validationErrors" data-container=".form-group"></validation-error>
</div>
